<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
	  <Description>Adds moderate Security and Password Solutions</Description>
	  <Authors>Joshua Gwynn-Palmer</Authors>
	  <Copyright>Joshua Gwynn-Palmer</Copyright>
	  <Version>1.0.0</Version>
	  <PackageId>JGP.Security</PackageId>
	  <PackageDescription>Adds moderate Security and Password Solutions</PackageDescription>
	  <RepositoryUrl>https://github.com/gwynnPalmer/JGP.Security</RepositoryUrl>
	  <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Cryptography.KeyDerivation" Version="6.0.6" />
    <PackageReference Include="Microsoft.AspNetCore.Identity" Version="2.2.0" />
  </ItemGroup>

</Project>
